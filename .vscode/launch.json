{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Rooter.Web",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/Rooter.Web/bin/Debug/net8.0/Rooter.Web.dll",
            "args": [],
            "cwd": "${workspaceFolder}/Rooter.Web",
            "stopAtEntry": false,
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            },
            "justMyCode": false,
            "requireExactSource": false
        },
        {
            "name": "Attach to Rooter.Web",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}